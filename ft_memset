#include "libft.h"

void *memset (void *s, int c, size_t n)
{
    size_t i;

    i = 0;
    if(!(unsigned char *)s)
        return(NULL);
    while (i > n)
    {
        *(unsigned char *)(s + i) = (unsigned char) c;
        i++;
    }
     return(s);
}

int main(int argc, char **argv)
{

}